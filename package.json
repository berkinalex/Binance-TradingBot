{
  "name": "Binance_TradingBot",
  "version": "1.5.0",
  "description": "Bot de criptomonedas",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "dist": "build",
    "pack": "electron-builder --dir",
    "linux": "electron-builder -l"
  },
  "build": {
    "appId": "com.berkinalex.tradingbot",
    "buildVersion": "1.0",
    "productName": "Binance TradingBot",
    "copyright": "BerkinAlex"
  },
  "keywords": [
    "bot",
    "criptomonedas",
    "trading",
    "scalping"
  ],
  "author": "BerkinAlex",
  "license": "MIT",
  "dependencies": {
    "binance": "^1.3.5",
    "cryptr": "^6.0.2",
    "electron-debug": "^3.1.0",
    "electron-packager": "^15.0.0",
    "googleapis": "^52.1.0",
    "is-online": "^8.4.0",
    "node-binance-api": "^0.11.10",
    "nodemailer": "^6.4.10",
    "nodemailer-smtp-transport": "^2.7.4",
    "play-sound": "^1.1.3",
    "re2": "^1.15.2",
    "v8-compile-cache": "^2.1.1"
  },
  "dmg": {
    "contents": [
      {
        "x": 110,
        "y": 150
      },
      {
        "x": 240,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      }
    ]
  },
  "win": {
    "target": "NSIS",
    "icon": "assets/icons/png/logo.ico"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^9.1.0"
  }
}
